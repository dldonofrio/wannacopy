{
		"name": "WannaCopy",
		"version": "1.2.4",
		"description": "Lets you copy things that a website might not want you to",
		"content_scripts": [
  			{
    		"matches": [
      			"<all_urls>"
    		],
    		"js": ["content.js"]
  			}
		],
		"permissions": [
			"activeTab",
			"http://*/*",
			"https://*/*"
		],
		"manifest_version": 2
}